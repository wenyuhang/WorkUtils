<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".main.MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >
        <com.wl.workutils.desgin.Label.EasyAutoLineView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >
            <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="跳转自定义标题栏"
            android:onClick="jumpTileBar"
            />
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="跳转图片资源查看"
                android:onClick="jumpImgLook"
                />
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="跳转Gallery"
                android:onClick="jumpGallery"
                />
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="跳转权限"
                android:onClick="jumpPermi"
                />

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="跳转VR"
                android:onClick="jumpVR"
                />
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="textview"
                android:onClick="jumpCusTextView"
                />
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Behavior"
                android:onClick="jumpBehavior"
                />
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="腾讯地图"
                android:onClick="jumpMap"
                />

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="chat"
                android:onClick="jumpChat"
                />
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="FFmpeg"
                android:onClick="jumpCompress"
                />
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="view"
                android:onClick="jumpCustomView"
                />
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="test"
                android:onClick="jumpTest"
                />
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="ScrollList"
                android:onClick="jumpScrollList"
                />

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="list Image"
                android:onClick="jumpListImage"
                />
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Bottom"
                android:onClick="jumpBottom"
                />
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="tab"
                android:onClick="jumpTab"
                />

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="tab2"
                android:onClick="jumpTab2"
                />

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="math"
                android:onClick="jumpMath"
                />

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Threadpool"
                android:onClick="jumpThreadPool"
                />
        </com.wl.workutils.desgin.Label.EasyAutoLineView>
    </LinearLayout>

</ScrollView>